body{
       background-color:#92ecef;
       padding:0px;
       font-family:sans-serif;
       margin: 0px;
} 
header{
  height: auto;
  padding:10px 0px 5px 0px;
  margin: 0px;
  text-align: center;
  text-weight: bold;
}

section{
  width:200px;
}
artical{
 width: 880px;
 font-family: Verdana, Geneva, sans-serif;

}
artical#middle p,artical#middle span{
 text-indent:2em;
}

#topleft{
  width: 200px;
  float: left;
  margin:0px 10px 0px 0px;
}

aside{
  width:200px;
}
#tableContainer{
  display: table;
  border-spacing: 10px;
}
#tableRow{
  display: table-row;
}
#left{
  display: table-cell;
  font-size: 120%;
  padding: 20px;
  vertical-align: top;
}
#middle{
  display: table-cell;
  background-color: #3ddaa1;
  padding: 20px;
  font-size:%130;
  vertical-align: top;
}
#right{
  display: table-cell;

  font-size: 120%;
  padding: 20px;
  vertical-align: top;
}
footer{
  text-align: center;
 /* background-color:#b196e3;*/
  margin: 0px;
  padding: 5px;
  font-size: %90;
}
small{
  margin: 0px;
  }
